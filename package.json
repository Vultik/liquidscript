{
  "name": "liquidscript",
  "version": "0.1.0-alpha.2",
  "author": "James",
  "license": "MIT",
  "homepage": "https://github.com/jg-rp/liquidscript",
  "bugs": {
    "url": "https://github.com/jg-rp/liquidscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jg-rp/liquidscript.git"
  },
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "format": "prettier --write ./src/**/*.ts",
    "format:check": "prettier --list-different ./src/**/*.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "clean": "rm -rf ./lib ./dist ./coverage",
    "benchmark": "npm run build && cd benchmark && node reference.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "main": "dist/liquidscript.cjs.js",
  "module": "dist/liquidscript.esm.js",
  "browser": "dist/liquidscript.iife.js",
  "browser-min": "dist/liquidscript.iife.min.js",
  "browser-bundle": "dist/liquidscript.iife.bundle.js",
  "browser-bundle-min": "dist/liquidscript.iife.bundle.min.js",
  "types": "dist/liquidscript.d.ts",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/he": "^1.1.2",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.21",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@yarnpkg/sdks": "^2.6.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.7",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "rewire": "^6.0.0",
    "rollup": "^2.70.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "decimal.js": "^10.3.1",
    "luxon": "^2.3.0"
  },
  "packageManager": "yarn@3.2.0"
}
