{
  "name": "liquid-air",
  "version": "1.0.0",
  "main": "lib/liquid.js",
  "author": "James",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ./src/**/*.ts",
    "format:check": "prettier --list-different ./src/**/*.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "clean": "rm -rf ./lib ./benchmark/*0x ./benchmark/*log",
    "benchmark": "npm run build && cd benchmark && node reference.js"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/he": "^1.1.2",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-plugin-jsdoc": "^37.9.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.7",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "rewire": "^6.0.0",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "decimal.js": "^10.3.1",
    "he": "^1.2.0",
    "luxon": "^2.3.0"
  }
}
